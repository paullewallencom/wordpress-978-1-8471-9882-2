slides_mc.stop();next_btn.addEventListener(MouseEvent.CLICK, goForward);prev_btn.addEventListener(MouseEvent.CLICK, goBack);function goForward(evt:Object):void{	if (slides_mc.currentFrame == slides_mc.totalFrames) {		slides_mc.gotoAndStop(1);	} else {	slides_mc.nextFrame();	}}function goBack(evt:Object):void{	if (slides_mc.currentFrame == 1) {		slides_mc.gotoAndStop(slides_mc.totalFrames);	} else {	slides_mc.prevFrame();	}}